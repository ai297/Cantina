{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "Default": "Host=localhost;Port=5432;Database=cantina_db;Username=user;Password=password"
  },
  "ServerVersion": "v0.2 (alpha)",
  "AuthOptions": {
    "LocalKey": "secret key",
    "AuthTokenLifetime": 6,
    "LongTokenLifetime": 4,
    "ActivationTokenLifetime": 5,
    "TokenIssuer": "theCantinaServer-0.1"
  },
  "ApiOptions": {
    "AllowedTags": [
      "user",
      "author",
      "smile"
    ],
    "MessagesBufferSize": 30
  },
  "IntevalsOptions": {
    "ArchiveSaving": 3,
    "OnlineUsersCheck": 5,
    "InactivityTime": 10,
    "UserCacheTime": 30
  }
}
